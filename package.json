  },
  "homepage": "https://mas-hub.vercel.app",
  "bugs": {
    "url": "https://github.com/yllvaranwar/mas-hub/issues"
  },
  "dependencies": {
    "cross-fetch": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
