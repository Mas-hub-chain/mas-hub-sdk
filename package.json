{
  "name": "mas-hub-sdk",
  "version": "1.0.0",
  "description": "Official TypeScript SDK for MAS Hub blockchain platform",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
