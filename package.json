{
  "name": "mas-hub-sdk",
  "version": "1.0.0",
  "description": "Official TypeScript SDK for MAS Hub blockchain platform",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "blockchain",
    "maschain",
    "smart-contracts",
    "tokenization",
    "enterprise",
    "sdk"
  ],
  "author": "MAS Hub Organization <contact@mashub.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yllvaranwar/mas-hub-sdk.git"
  },
  "homepage": "https://mas-hub.vercel.app",
  "bugs": {
    "url": "https://github.com/yllvaranwar/mas-hub/issues"
  },
  "dependencies": {
    "cross-fetch": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.6.0",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  }
}
